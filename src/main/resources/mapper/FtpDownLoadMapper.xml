<mapper namespace="com.qzkj.demo.mapper.FtpDownLoadMapper">

    <insert id="addDownLoad" parameterType="com.qzkj.demo.pojo.FtpDownLoad">
      insert into ftpdownloadbean(eNB,avgSpeed,maxSpeed,testTime)
      values (#{eNB},#{avgSpeed},#{maxSpeed},#{testTime})
    </insert>

</mapper>
