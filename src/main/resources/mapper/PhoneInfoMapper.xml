<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qzkj.demo.mapper.PhoneInfoMapper" >

    <insert id="addPhoneInfo" parameterType="com.qzkj.demo.pojo.PhoneInfo">
      insert into phoneinfo(code,dataAtivity,dataModel,imei,imsi,internetType,phoneName,phoneType,systemm)
      values (#{code},#{dataAtivity},#{dataModel},#{imei},#{imsi},#{internetType},#{phoneName},#{phoneType},#{systemm})
    </insert>

</mapper>
